; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Object",
"class": "BetaflightHelper",
"language": "GDScript",
"path": "res://utils/BetaflightHelper.gd"
}, {
"base": "ResourceFormatLoader",
"class": "HTerrainDataLoader",
"language": "GDScript",
"path": "res://addons/zylann.hterrain/hterrain_resource_loader.gd"
}, {
"base": "ResourceFormatSaver",
"class": "HTerrainDataSaver",
"language": "GDScript",
"path": "res://addons/zylann.hterrain/hterrain_resource_saver.gd"
}, {
"base": "Object",
"class": "InputFrame",
"language": "GDScript",
"path": "res://utils/InputFrame.gd"
}, {
"base": "Reference",
"class": "Mode",
"language": "GDScript",
"path": "res://utils/Mode.gd"
}, {
"base": "Object",
"class": "PID",
"language": "GDScript",
"path": "res://flight/PID.gd"
}, {
"base": "RigidBody",
"class": "QuadEntity",
"language": "GDScript",
"path": "res://entities/QuadEntity.gd"
}, {
"base": "Control",
"class": "RecieverDisplay",
"language": "GDScript",
"path": "res://ui/RecieverDisplay.gd"
} ]
_global_script_class_icons={
"BetaflightHelper": "",
"HTerrainDataLoader": "",
"HTerrainDataSaver": "",
"InputFrame": "",
"Mode": "",
"PID": "",
"QuadEntity": "",
"RecieverDisplay": ""
}

[application]

config/name="QuadCam"
run/main_scene="res://scenes/Config.tscn"
config/icon="res://icon.png"

[autoload]

RC="*res://flight/RC.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/silicon.vfx.lens_flare/plugin.cfg", "res://addons/zylann.hterrain/plugin.cfg" )

[physics]

common/physics_jitter_fix=0.0
common/enable_pause_aware_picking=true
3d/physics_engine="Bullet"
3d/godot_physics/use_bvh=false

[rendering]

threads/thread_model=2
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
